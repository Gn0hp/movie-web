{{#each ptitDataArr}}
<div class="post-container">
    <a href="{{this.link}}">
    <div class="movie-list-item-img">
        <img
        src="{{ this.img }}"
        alt=""
        class="movie-list-item-img"
        />
        <span class="post-title">{{ this.title }}</span>
        <p class="movie-list-item-desc">
        {{ this.des }}
        </p>
        <a href="">
        <button class="movie-list-item-btn">Watch now</button></a>
    </div>
</a>
</div>
{{/each}}